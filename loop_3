#include <stdio.h>

int main()
{
    int a, b, c, i, digits;

    printf("Enter number of digits: ");
    scanf("%d", &terms);

    a = 0;
    b = 1;
    c = 0;

    printf("Fibonacci series: \n");

    for(i=1; i<=digits; i++)
    {
        printf("%d, ", c);

        a = b;
        b = c;
        c = a + b;
