#include <stdio.h>

int main()
{
    int n,arr[n],sum=0;
    printf("Enter the size of array : ");
    scanf("%d", &n);
    
    for(int i=0; i<n; i++){
        printf("Enter the %d element of array : ",i);
        scanf("%d", &arr[i]);
    }
    
    for(int i=0; i<n; i++){
        sum = sum + arr[i];
    }
    printf("Sum of elements of array : %d", sum);
    

    return 0;
}

