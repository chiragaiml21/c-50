#include <stdio.h>

int main()
{
    int a,b;
    printf("Enter the number a: ");
    scanf("%d", &a);
    printf("Enter the number b: ");
    scanf("%d", &b);
    
    switch(a>b){
        case 1:
        printf("%d is greater than %d",a,b);
        break;
        
        case 0:
        switch(b>a){
            case 1:
            printf("%d is greater than %d",b,a);
            break;
            
            case 0:
            printf("a is equal to b");
            break;
        }
    }

    return 0;
}

