#include <stdio.h>

int main()
{
    int n,arr[n],max,min;
    printf("Enter the size of array : ");
    scanf("%d", &n);
    
    for(int i=0; i<n; i++){
        printf("Enter the %d element of array : ",i);
        scanf("%d", &arr[i]);
    }
    max = arr[0];
    for(int i=0; i<n; i++){
        if(max<arr[i]){
            max = arr[i];
        }
    }
    printf("Maximum element of array : %d", max);
    

    return 0;
}

