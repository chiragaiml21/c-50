#include <stdio.h>

int main()
{
    int n,arr[n];
    printf("Enter the size of array : ");
    scanf("%d", &n);
    
    for(int i=0; i<n; i++){
        printf("Enter the %d element of array : ",i);
        scanf("%d", &arr[i]);
    }
    printf("All negative elements of array : ");
    for(int i=0; i<n; i++){
        if(arr[i]<0){
            printf("%d, ", arr[i]);
        }
    }

    return 0;
}


