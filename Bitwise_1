#include <stdio.h>

int main()
{
    int a, b;

    printf("Enter any two numbers: ");
    scanf("%d%d", &a, &b);

    printf("Value of a before swapping = %d\n", a);
    printf("Value of b after swapping = %d\n", b);

    a ^= b;
    b ^= a;
    a ^= b;

    printf("Value of a after swapping = %d\n", a);
    printf("Value of b after swapping = %d\n", b);

    return 0;
}
