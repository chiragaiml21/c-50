#include <stdio.h>

int main()
{
    int n,arr[n];
    printf("Enter the size of array : ");
    scanf("%d", &n);
    
    for(int i=0; i<n; i++){
        printf("Enter the %d element of array : ",i);
        scanf("%d", &arr[i]);
    }
    
    for(int i=0; i<n; i++){
        
        if(i==(n-1)){
            printf("%d\n", arr[i]);
        }
        else{
            printf("%d, ",arr[i]);
        }
    }

    return 0;
}

