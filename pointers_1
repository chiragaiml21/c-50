#include <stdio.h>

int main()
{
    int arr[5], *p=arr;
    
    printf("Enter the elements : ");
    for(int i=0; i<5; i++){
        scanf("%d", p);
        p++;
    }
    
    p = arr;
    
    printf("You entered : ");
    for(int i=0; i<5; i++){
        printf("%d", *p);
        p++;
    }

    return 0;
}
