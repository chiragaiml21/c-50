#include <stdio.h>

int main()
{
    int x, num, reverse=0;
    printf("Enter the number : ");
    scanf("%d", &x);
    
    num = x;
    
    while(x!=0){
        reverse = (reverse*10)+(x%10);
        
        x = x/10;
    }
    
    if(reverse==num){
      printf("Palendrom");  
    }
    
    else
    printf("no");

    return 0;
}
